  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@types/jest": "^29.5.14",
    "@types/systemjs": "^6.1.1",
    "@types/webpack-env": "^1.18.5",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "js-cookie": "^3.0.5",
    "single-spa": "^6.0.3"
  },
  "types": "dist/Truck-root-config.d.ts"
